  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "eslint-plugin-jsdoc": "^39.2.9",
    "nodemon": "^2.0.16"
  },
  "dependencies": {
    "glob": "^8.0.3"
  }
}
