{
  "name": "plugin-dev-utilities",
  "type": "module",
  "scripts": {
    "init": "node init.js",
    "once:star": "node combine.js ./j/star ../plugins/j/abs/ext J-ABS-Star.js",
    "watch:star": "nodemon -L combine.js ./j/abs/ext/star ../plugins/j/jabs/ext J-ABS-Star.js",
